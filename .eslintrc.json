{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
	"parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off",
	  "no-unused-vars": "warn",
    "import/extensions": "off",
    "object-shorthand": "off",
    "no-new": "off",
  }
}
