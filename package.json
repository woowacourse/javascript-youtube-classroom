    "url": "https://github.com/al-bur/javascript-youtube-classroom/issues"
  },
  "homepage": "https://github.com/al-bur/javascript-youtube-classroom#readme",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "commitizen": "^4.2.4",
    "css-loader": "^6.7.0",
    "cypress": "^9.5.2",
    "cypress-localstorage-commands": "^1.7.0",
    "cz-customizable": "^6.3.0",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
